#!/bin/bash
echo "--------------------------------------------------------------------------------------------------------------------------------------------------"
iptables -L DYNRULE -n -v --line-numbers
echo "--------------------------------------------------------------------------------------------------------------------------------------------------"
iptables -L vlan0655_In_RULE_0 -n -v --line-numbers
echo "--------------------------------------------------------------------------------------------------------------------------------------------------"
echo "Process Info:"
echo "USER       PID %CPU %MEM    VSZ   RSS TTY      STAT START   TIME COMMAND"
ps axu | grep authgwd | grep -v "/bin/bash" | grep -v grep
echo "--------------------------------------------------------------------------------------------------------------------------------------------------"
echo "Active sockets:"
netstat -antp | grep authgwd
echo "--------------------------------------------------------------------------------------------------------------------------------------------------"
